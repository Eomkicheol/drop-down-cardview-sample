platform :ios, '9.0'

target 'RxSwiftExample' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  inhibit_all_warnings!
  
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxViewBinder'
  pod 'SnapKit', '~> 4.0.0'

end
